<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LineWrapContainer">
        <attr name="horizontalSpace" format="dimension" />
        <attr name="verticalSpace" format="dimension" />
        <attr name="maxLines" format="integer" />
    </declare-styleable>

    <declare-styleable name="ExpandableTextView">
        <attr name="maxCollapsedLines" format="integer"/>
        <attr name="animDuration" format="integer"/>
        <attr name="animAlphaStart" format="float"/>
        <attr name="expandIndicator" format="reference"/>
        <attr name="collapseIndicator" format="reference"/>
        <attr name="expandToggleType" format="enum">
            <enum name="ImageButton" value="0"/>
            <enum name="TextView" value="1"/>
        </attr>
        <attr name="expandableTextId" format="reference"/>
        <attr name="expandCollapseToggleId" format="reference"/>
        <attr name="expandToggleOnTextClick" format="boolean"/>
    </declare-styleable>


    <declare-styleable name="BatteryView">
        <attr name="powerValue" format="fraction"/>
    </declare-styleable>

    <attr name="buttonTextColor" format="reference" />
    <attr name="buttonIcon" format="reference" />


    <attr name="brandColor" format="reference" />
    <attr name="brandDarkColor" format="reference" />
    <attr name="brandBlackColor" format="reference" />
    <attr name="brandGrayColor" format="reference" />

    <attr name="warnColor" format="reference" />
    <attr name="successColor" format="reference" />

    <attr name="textColor0" format="reference" />
    <attr name="textColor1" format="reference" />
    <attr name="textColor2" format="reference" />
    <attr name="textColor3" format="reference" />
    <attr name="bg0" format="reference" />
    <attr name="bg1" format="reference" />
    <attr name="line" format="reference" />



    <attr name="btnBigNormalColor" format="reference" />
    <attr name="btnBigPressColor" format="reference" />
    <attr name="btnBigDisableColor" format="reference" />
    <attr name="btnBigNormalTextColor" format="reference" />
    <attr name="btnBigPressTextColor" format="reference" />
    <attr name="btnBigDisableTextColor" format="reference" />

    <attr name="btnMiddleNormalColor" format="reference" />
    <attr name="btnMiddlePressColor" format="reference" />
    <attr name="btnMiddleDisableColor" format="reference" />
    <attr name="btnMiddleNormalTextColor" format="reference" />
    <attr name="btnMiddlePressTextColor" format="reference" />
    <attr name="btnMiddleDisableTextColor" format="reference" />

    <attr name="btnMiddleNormalColor1" format="reference" />
    <attr name="btnMiddlePressColor1" format="reference" />
    <attr name="btnMiddleDisableColor1" format="reference" />
    <attr name="btnMiddleNormalTextColor1" format="reference" />
    <attr name="btnMiddlePressTextColor1" format="reference" />
    <attr name="btnMiddleDisableTextColor1" format="reference" />

    <attr name="btnSmallNormalColor" format="reference" />
    <attr name="btnSmallPressColor" format="reference" />
    <attr name="btnSmallDisableColor" format="reference" />
    <attr name="btnSmallNormalTextColor" format="reference" />
    <attr name="btnSmallPressTextColor" format="reference" />
    <attr name="btnSmallDisableTextColor" format="reference" />
    <attr name="btnSmallNormalBorderColor" format="reference" />
    <attr name="btnSmallPressBorderColor" format="reference" />
    <attr name="btnSmallDisableBorderColor" format="reference" />

</resources>